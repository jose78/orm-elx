<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">
 
 
 	
	
	<context:component-scan base-package="com.rapid.arq.server" />
 	<bean id="RapidContext" class="com.rapid.arq.server.RapidContext"/>
    <bean id="FactoryService" class="com.rapid.arq.server.FactoryService"/>
	<bean id="ServiceRenameStrategy" class="com.rapid.arq.server.service.impl.RenameContractStrategyServiceImpl"/> 		 	
 	<bean id="ServiceRenameTactic" class="com.rapid.arq.server.service.impl.RenameContractTacticServiceImpl"/>
 	
 
 	<bean id="ServiceRename" factory-bean="FactoryService" factory-method="getRename" />
 	
 	
	
 
</beans>